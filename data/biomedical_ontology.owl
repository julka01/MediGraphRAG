<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/ontologies/biomedical#"
     xml:base="http://www.semanticweb.org/ontologies/biomedical"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#">
    <owl:Ontology rdf:about="http://www.semanticweb.org/ontologies/biomedical"/>

    <!-- Core Biomedical Classes -->
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#Disease">
        <rdfs:label>Disease</rdfs:label>
        <rdfs:comment>A pathological condition affecting an organism</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#Treatment">
        <rdfs:label>Treatment</rdfs:label>
        <rdfs:comment>Medical intervention to cure or manage disease</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#Medication">
        <rdfs:label>Medication</rdfs:label>
        <rdfs:comment>Pharmaceutical drug used for treatment</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#Procedure">
        <rdfs:label>Procedure</rdfs:label>
        <rdfs:comment>Medical procedure or intervention</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#Symptom">
        <rdfs:label>Symptom</rdfs:label>
        <rdfs:comment>Observable manifestation of disease</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#Biomarker">
        <rdfs:label>Biomarker</rdfs:label>
        <rdfs:comment>Measurable biological indicator</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#AnatomicalStructure">
        <rdfs:label>AnatomicalStructure</rdfs:label>
        <rdfs:comment>Body part or organ system</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#RiskFactor">
        <rdfs:label>RiskFactor</rdfs:label>
        <rdfs:comment>Factor that increases disease probability</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#Stage">
        <rdfs:label>Stage</rdfs:label>
        <rdfs:comment>Disease progression stage</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#Gene">
        <rdfs:label>Gene</rdfs:label>
        <rdfs:comment>Genetic element</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#Protein">
        <rdfs:label>Protein</rdfs:label>
        <rdfs:comment>Protein molecule</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#Pathway">
        <rdfs:label>Pathway</rdfs:label>
        <rdfs:comment>Biological pathway or process</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#ClinicalTrial">
        <rdfs:label>ClinicalTrial</rdfs:label>
        <rdfs:comment>Research study with human participants</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#Guideline">
        <rdfs:label>Guideline</rdfs:label>
        <rdfs:comment>Clinical practice guideline</rdfs:comment>
    </owl:Class>
    
    <owl:Class rdf:about="http://www.semanticweb.org/ontologies/biomedical#Organization">
        <rdfs:label>Organization</rdfs:label>
        <rdfs:comment>Medical or research organization</rdfs:comment>
    </owl:Class>

    <!-- Object Properties (Relationships) -->
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#TREATS">
        <rdfs:label>TREATS</rdfs:label>
        <rdfs:comment>Treatment treats disease</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#CAUSES">
        <rdfs:label>CAUSES</rdfs:label>
        <rdfs:comment>Factor causes condition</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#SYMPTOM_OF">
        <rdfs:label>SYMPTOM_OF</rdfs:label>
        <rdfs:comment>Symptom manifests in disease</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#BIOMARKER_FOR">
        <rdfs:label>BIOMARKER_FOR</rdfs:label>
        <rdfs:comment>Biomarker indicates disease</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#LOCATED_IN">
        <rdfs:label>LOCATED_IN</rdfs:label>
        <rdfs:comment>Entity located in anatomical structure</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#AFFECTS">
        <rdfs:label>AFFECTS</rdfs:label>
        <rdfs:comment>Disease affects anatomical structure</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#HAS_STAGE">
        <rdfs:label>HAS_STAGE</rdfs:label>
        <rdfs:comment>Disease has progression stage</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#RISK_FACTOR_FOR">
        <rdfs:label>RISK_FACTOR_FOR</rdfs:label>
        <rdfs:comment>Factor increases disease risk</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#DIAGNOSES">
        <rdfs:label>DIAGNOSES</rdfs:label>
        <rdfs:comment>Procedure diagnoses disease</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#DETECTS">
        <rdfs:label>DETECTS</rdfs:label>
        <rdfs:comment>Procedure detects condition</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#PRODUCES">
        <rdfs:label>PRODUCES</rdfs:label>
        <rdfs:comment>Structure produces substance</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#ENCODES">
        <rdfs:label>ENCODES</rdfs:label>
        <rdfs:comment>Gene encodes protein</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#PARTICIPATES_IN">
        <rdfs:label>PARTICIPATES_IN</rdfs:label>
        <rdfs:comment>Entity participates in pathway</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#ASSOCIATED_WITH">
        <rdfs:label>ASSOCIATED_WITH</rdfs:label>
        <rdfs:comment>General association between entities</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#CONTRAINDICATED_FOR">
        <rdfs:label>CONTRAINDICATED_FOR</rdfs:label>
        <rdfs:comment>Treatment contraindicated for condition</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#GENETIC_RISK_FACTOR">
        <rdfs:label>GENETIC_RISK_FACTOR</rdfs:label>
        <rdfs:comment>Genetic variant increases disease risk</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#RECOMMENDS">
        <rdfs:label>RECOMMENDS</rdfs:label>
        <rdfs:comment>Guideline recommends treatment</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#STUDIES">
        <rdfs:label>STUDIES</rdfs:label>
        <rdfs:comment>Clinical trial studies treatment</rdfs:comment>
    </owl:ObjectProperty>
    
    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/ontologies/biomedical#PUBLISHED_BY">
        <rdfs:label>PUBLISHED_BY</rdfs:label>
        <rdfs:comment>Guideline published by organization</rdfs:comment>
    </owl:ObjectProperty>
</rdf:RDF>
